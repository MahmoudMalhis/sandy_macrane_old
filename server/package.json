{
  "name": "sandy-macrame-server",
  "version": "1.0.0",
  "description": "Backend API for Sandy Macrame website",
  "main": "src/server.js",
  "type": "module",
  "scripts": {
    "start": "node src/server.js",
    "dev": "nodemon src/server.js",
    "migrate": "knex migrate:latest --knexfile ./knexfile.js",
    "rollback": "knex migrate:rollback --knexfile ./knexfile.js",
    "make:migration": "knex migrate:make --knexfile ./knexfile.js",
    "make:seed": "knex seed:make --knexfile ./knexfile.js"
  },
  "dependencies": {
    "@sendgrid/mail": "^8.1.6",
    "bcrypt": "^6.0.0",
    "cloudinary": "^1.41.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "knex": "^3.0.1",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "mysql2": "^3.6.5",
    "node-fetch": "^3.3.2",
    "nodemailer": "^7.0.6",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "nodemon": "^3.0.1"
  },
  "keywords": [
    "macrame",
    "api",
    "express",
    "mysql"
  ],
  "author": "Sandy Macrame",
  "license": "MIT"
}
